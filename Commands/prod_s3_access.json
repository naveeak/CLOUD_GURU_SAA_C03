{
    "Version": "2012-10-17",
    "Statement": [
        {
                "Sid": "StatementToReadAllTheS3Buckets",
                "Action": ["s3:ListAllMyBuckets", "s3:GetBucketLocation"],
                "Effect": "Allow",
                "Resource": ["arn:aws:s3:::*"]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": [
                "arn:aws:s3:::cfst-3035-ef6aee913fb550f63eadabe7c9-s3bucketprod-c0su4vfqw9ow/*",
                "arn:aws:s3:::cfst-3035-ef6aee913fb550f63eadabe7c9-s3bucketprod-c0su4vfqw9ow"
            ]
        }
    ]
}
